        if (place_meeting(x,y + 8,objTrailmap)) &amp;&amp; (!place_meeting(x,y + 8,objTrail)) south = 3;
        else south = -1;
        direct = choose(east,north,west,south);
        switch (direct) {
            case 0: if (!instance_place(x + 8,y,objTrailDeadend)) instance_create(x + 8,y,objTrailDeadend); break;
            case 1: if (!instance_place(x,y - 8,objTrailDeadend)) instance_create(x,y - 8,objTrailDeadend); break;
            case 2: if (!instance_place(x - 8,y,objTrailDeadend)) instance_create(x - 8,y,objTrailDeadend); break;
            case 3: if (!instance_place(x,y + 8,objTrailDeadend)) instance_create(x,y + 8,objTrailDeadend); break;
        }
    }
}
