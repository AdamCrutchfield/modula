            <string>//Dead ends
if (!instance_exists(objTrailblazer)) &amp;&amp; (createDeadend) &amp;&amp; (alarm[0] == -1) {
    createDeadend = false;
    var coinflip = irandom(1);
    if (coinflip == 1) {
        if (place_meeting(x + 8,y,objTrailmap)) &amp;&amp; (!place_meeting(x + 8,y,objTrail)) east = 0;
        else east = -1;
        if (place_meeting(x,y - 8,objTrailmap)) &amp;&amp; (!place_meeting(x,y - 8,objTrail)) north = 1;
